<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <flow-definition id="retencaoflow">
        <view id="List">
            <vdl-document>/retencaoflow/List.xhtml</vdl-document>
        </view>

        <start-node>List</start-node>

        <flow-return id="Faturamento">
            <from-outcome>/contratoflow/FaturamentoEdit.xhtml</from-outcome>
        </flow-return>
        <flow-return id="homepage">
            <from-outcome>/index.xhtml</from-outcome>
        </flow-return>
        <inbound-parameter>
            <name>faturamento</name>
            <value>#{retencaoFlow.controller.faturamento}</value>
        </inbound-parameter>
    </flow-definition>
</faces-config>
